{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=10000"
        }]
      }, {
        "source": "**/*.@(js)",
        "headers": [{
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }, {
          "key": "Cache-Control",
          "value": "max-age=10000"
        }]
      },
      {
        "source": "**/*.@(css)",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=10000"
        }]
      },{
        "source": "/v1/**",
        "headers": [{
          "key": "Cache-Control",
          "value": "max-age=10000"
        }]
      }]
  }
}