{
  "hosting": {
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [{
        "source": "growthfile-testing.firebaseapp.com/v1/**",
        "destination": "/v1/index.html"
      },{
        "source": "growthfile-testing.firebaseapp.com",
        "destination": "/index.html"
      }],
      "headers": [{
          "source": "v1/**",
          "headers": [{
            "key": "Cache-Control",
            "value": "max-age=0"
          }]
      }]
    }

}