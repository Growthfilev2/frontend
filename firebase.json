{
  "hosting": {
    "public": "src",
    "ignore": [
      "firebase.json",
      "*/.",
      "/node_modules/"
    ],
    "cleanUrls":true,
    "headers": [
      {
        "source":"/**",
        "headers": [{
          "key": "Cache-Control",
          "value": "no-cache"
        }]
      }
    ]
  }
}
